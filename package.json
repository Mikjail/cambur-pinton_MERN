{
  "name": "camburpinton-dev",
  "version": "1.0.0",
  "description": "cambur pinton",
  "main": "index.js",
  "engines": {
    "node": "20.x",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node -r dotenv/config index.js",
    "server": "node index.js",
    "startDev": "nodemon index.js",
    "client": "npm run start:dev --prefix client",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "gh-build": "npm install --prefix client && npm run build --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.3",
    "concurrently": "3.6.0",
    "connect-flash": "0.1.1",
    "cookie-session": "2.0.0-beta.3",
    "dotenv": "^16.3.1",
    "express": "4.16.3",
    "mercadopago": "^1.0.19",
    "mongoose": "^7.4.3",
    "nodemon": "1.17.5",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "sendgrid": "5.2.3"
  }
}
